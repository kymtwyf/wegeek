/* miniprogram/pages/others/pages.wxss */
.page-body {
  overflow: hidden;
  background:linear-gradient(-45deg,rgba(141,149,172,1),rgba(160,169,186,1));
}
.pages-container {
  position: relative;
  margin-top: 28rpx;
  width: 750rpx;
  height: 896rpx;

  padding: 148rpx 98rpx 0 98rpx;
  box-sizing: border-box;
}

.pages-container > .background {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.my-book-container {
  width: 100%;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: center;
}
.my-book {
  width: 686rpx;
  height: 192rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 0px 12rpx 0px rgba(149,137,130,1);
  border-radius:30rpx 30rpx 0 0 ;
  line-height: 192rpx;
  text-align: center;
}
